<%= form_tag({controller: "git_hub", action: "exportFiles"}) do %>
    <% @stored_files.each do |file| %>
      <div class="panel panel-primary panel-default">
        <div class="panel-heading">
          <div class="row">
            <div class="col-md-6">
             Module's files
            </div>
          </div>
        </div>
          <table class="table borderless table-condensed">
            <thead>
              <th>File name</th>
              <th>Repository</th>
              <th>Edit</th>
              <th>Delete</th>
            </thead>
            <tbody>
            <tr>
              <td><%= file.name%></td>
              <td><%= file.repo%></td>
              <td><button class="btn btn-primary btn-xs" data-title="Edit" data-toggle="modal" data-target="#edit" ><i class="fa fa-pencil" aria-hidden="true"></i></button></td>
              <td><button class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete" ><i class="fa fa-trash" aria-hidden="true"></i></button></td>
            </tr>
            <!--
            <file.lines.each do |line| %>
                < if !line[:contenido].nil? %>
                    <tr>
                      <td style="border-top:0px;">< line[:numero] %></td>
                      <td style="white-space: pre-wrap; border-top:0px;">< line[:contenido]%></td>

                      <!--td style="border-top:0px;">
                      <%#= hidden_field_tag 'lines[][numero]', cont%>
                    <%#= check_box_tag 'lines[][contenido]', line, false,{:id => cont, :class=> "checkboxLine"} %>
                  </td>
                </tr>
                <end%>
            <end%>
            -->
            </tbody>
          </table>

      </div>
    <% end %>
    <%= submit_tag("Create module", :class=>'btn btn-primary') %>
<%end%>